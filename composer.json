{
  "name": "nekofar/hiweb-cli",
  "type": "project",
  "description": "Simple command-line tool to access HiWeb account information",
  "license": "MIT",
  "authors": [
    {
      "name": "Milad Nekofar",
      "email": "milad@nekofar.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "symfony/console": "^5.3",
    "symfony/panther": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "nekofar/dev-tools": "^1.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Nekofar\\HiWeb\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
