{
  "name": "nekofar/hiweb-cli",
  "type": "project",
  "description": "Simple command-line tool to access HiWeb account information",
  "license": "MIT",
  "authors": [
    {
      "name": "Milad Nekofar",
      "email": "milad@nekofar.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/console": "^5.3",
    "symfony/panther": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "nekofar/dev-tools": "^2.1"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "composer/installers": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Nekofar\\HiWeb\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "phpcbf": "phpcbf",
    "phpcs": "phpcs",
    "phplint": "php-parallel-lint"
  },
  "bin": [
    "bin/hiweb"
  ]
}
